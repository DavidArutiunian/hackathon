{
  "name": "hackathon",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "pretty": "prettier --write src",
    "round": "ts-node -r dotenv/config -O '{\"module\": \"commonjs\"}' src/index.ts",
    "status:round1": "env $(cat .env) sh -c 'http GET https://datsanta.dats.team/api/round/${0} X-API-Key:$TOKEN'",
    "status:round2": "env $(cat .env) sh -c 'http GET https://datsanta.dats.team/api/round2/${0} X-API-Key:$TOKEN'"
  },
  "dependencies": {
    "@react-hook/window-size": "^3.1.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.17",
    "axios": "^1.2.1",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
